{
    "manifest_version": 3,
    "name": "LinkedIn Auto Connect",
    "version": "1.0",
    "description": "Automatically sends connection requests on LinkedIn search results Created by Ankur Jaiswal",
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.linkedin.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
      "activeTab",
      "scripting"
    ]
  }
